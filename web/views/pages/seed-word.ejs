<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head',{title}); %>
  </head>
  <body>
    <%- include('../partials/nav'); %>
    <main>
      <header>Word Seed Generator</header>
      <section>
        <p>Pick four words from the following</p>
        <form>
          <table>
            <% rows.forEach((row)=>{ %>
            <tr>
              <% row.forEach((word)=>{%>
              <td>
                <input type="checkbox" id="<%=word%>" value="<%=word%>" />
                <label for="<%=word%>"> <%=word%> </label>
              </td>
              <%})%>
            </tr>
            <%})%>
          </table>
          <input type="submit" value="Submit" />
        </form>
        <p></p>
      </section>
    </main>
  </body>
</html>
